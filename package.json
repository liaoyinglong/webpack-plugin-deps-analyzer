{
  "name": "webpack-plugin-deps-analyzer",
  "scripts": {
    "prebuild": "tsc --diagnostics",
    "build": "tsup --clean --splitting",
    "build-fast": "pnpm run build --no-dts",
    "dev": "pnpm run build-fast --watch",
    "test": "vitest run",
    "test:u": "vitest -u",
    "release": "pnpm build"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "vitest": "^1.4.0",
    "webpack": "^5.91.0",
    "@types/fs-extra": "^11.0.4",
    "typescript": "5.4.3"
  },
  "packageManager": "pnpm@8.14.1",
  "dependencies": {
    "fs-extra": "^11.2.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liaoyinglong/webpack-plugin-deps-analyzer.git"
  }
}